{
  "name": "web-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.0.0 <21.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "copy:prisma": "node scripts/prisma/copy-prisma-to-dist.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "daemon": "npm run build && pm2 start dist/main.js -i max --name web-server",
    "daemon:re": "npm run build && pm2 stop web-server && pm2 start dist/main.js -i max --name web-server",
    "daemon:stop": "pm2 stop web-server",
    "daemon:log": "pm2 logs web-server --lines 1000",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:cov": "jest --coverage --passWithNoTests",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest",
    "prisma": "rimraf prisma/basic && prisma generate && npm run postgenerate",
    "prisma:migrate": "prisma migrate dev",
    "generate:prisma-types": "ts-node scripts/prisma/generate-prisma-types.ts",
    "postgenerate": "npm run generate:prisma-types",
    "start:test-db": "ts-node -r tsconfig-paths/register ./scripts/prisma/test-db-setup.dev.ts",
    "start:test-app": "nest start",
    "start:test": "ts-node -r tsconfig-paths/register ./scripts/prisma/test-db-setup.dev.ts --start-app",
    "db:seed": "ts-node -r tsconfig-paths/register ./scripts/prisma/seed.dev/index.ts"
  },
  "dependencies": {
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@nestjs/apollo": "^13.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/websockets": "^11.1.8",
    "@prisma/client": "^6.19.0",
    "@types/socket.io": "^3.0.2",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dayjs": "^1.11.18",
    "graphql": "^16.11.0",
    "graphql-parse-resolve-info": "^4.14.1",
    "image-size": "^2.0.2",
    "mysql2": "^3.15.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.4",
    "qrcode-generator": "^2.0.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@testcontainers/postgresql": "^11.7.2",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.4",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.7.2",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^prisma/(.*)$": "<rootDir>/../prisma/$1",
      "^axios$": "axios/dist/node/axios.cjs"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
